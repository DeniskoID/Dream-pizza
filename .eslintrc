{
  "extends": [
    "eslint:recommended",   
    "plugin:react/recommended",
    "plugin:react/jsx-runtime", 
    "prettier"  
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "root": true,
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "warn"
    // "react/prop-types": "off"
    // "react/jsx-uses-vars": "error",
    // "react/jsx-uses-react": "error"
  }
}
